<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smart.search.mapper.OrderMapper">

    <insert id="insertOrder">
        INSERT INTO `order` ( `id`, `user_id`, `product_id`, `product`, `price` )
        VALUES
	        ( #{id}, #{userId}, #{productId}, #{product}, #{price});
    </insert>
</mapper>